{
  "MODE": "production",

  "DATABASE": {
    "OPTIONS": {
      "logging": false
    }
  },

  "LOGGING": {
    "appenders": [
      {
        "type": "file",
        "filename": "/home/pushserver/logs/pushserver/push_server",
        "absolute": true,
        "maxLogSize": 20480,
        "backups": 10,
        "category": "server"
      },
      {
